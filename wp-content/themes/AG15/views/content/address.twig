{# {{ dump(slide) }} #}
<ul class="address">
	<li class="name"><h2>{{ address.name }}</h2></li>
	<li>{{ address.line_1 }}</li>	
	{# <li>{{ address.line_2 }}</li> #}
	<li>{{ address.city }}, {{ address.state }} {{ address.zip }}</li>
	<li class="phone"><span>T</span> {{ phone }}</li>
	<li class="fax"><span>F</span> {{ fax }}</li>
</ul>